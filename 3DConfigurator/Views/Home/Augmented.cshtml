<!doctype HTML>
<html>

<script src="https://aframe.io/releases/0.6.0/aframe.min.js"></script>
<script src="https://cdn.rawgit.com/jeromeetienne/AR.js/1.5.0/aframe/build/aframe-ar.js" "></script>
<body style='margin : 0px; overflow: hidden;'>
    <a-scene embedded arjs="debugUIEnabled: false;">
        <!-- below is for content -->
        <a-assets>
            <!-- id=“file name“ src=“file url" -->
            <a-asset-item id="Current" src="https://handbaldesselgem.be.iis.websrv.be/wwwroot/objects/current.gltf"></a-asset-item>
        </a-assets>
        <!-- scale values allow you to adjust the file’s display size, gltf-model=“#your file name“ -->
        <a-entity scale="1 1 1" gltf-model="#Current"></a-entity>
        <a-marker-camera preset='hiro'></a-marker-camera>
    </a-scene>
</body>





</html>